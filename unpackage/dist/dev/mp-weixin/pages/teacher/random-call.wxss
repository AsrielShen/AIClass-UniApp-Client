#random-call {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: #F6F6F6;
}
#random-call .random-block {
  padding: 0 30rpx;
  border-bottom: 1rpx solid #cbcbcb;
  background-color: #fff;
}
#random-call .random-block .random-action {
  margin: 20rpx 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
#random-call .random-block .random-action .action-name {
  display: flex;
  align-items: center;
}
#random-call .random-block .random-action .action-name::before {
  content: 'â€¢';
  font-size: 40rpx;
  color: #F37B1D;
}
#random-call .random-block .random-action .action-button {
  margin: 0;
  font-size: 24rpx;
  border-radius: 25rpx;
  color: #fff;
  background-color: #39c5bb;
}
